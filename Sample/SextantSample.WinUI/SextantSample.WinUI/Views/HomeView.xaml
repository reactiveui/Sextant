<?xml version="1.0" encoding="UTF-8" ?>
<views:ReactivePageHomeView
    x:Class="SextantSample.WinUI.Views.HomeView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="using:SextantSample.WinUI.Views">
    <views:ReactivePageHomeView.Content>
        <StackPanel>
            <TextBlock>Home</TextBlock>
            <Button x:Name="FirstModalButton" Content="First Modal" />
            <Button x:Name="PushPage" Content="Push Red Page" />
            <Button x:Name="PushGenericPage" Content="Push Green Page" />
        </StackPanel>
    </views:ReactivePageHomeView.Content>
</views:ReactivePageHomeView>
