<?xml version="1.0" encoding="UTF-8" ?>
<views:ReactivePageRedView
    x:Class="SextantSample.WinUI.Views.RedView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:views="using:SextantSample.WinUI.Views">
    <views:ReactivePageRedView.Content>
        <StackPanel Background="Red">
            <TextBlock>Red</TextBlock>
            <Button
                x:Name="PopModal"
                Content="Pop Modal"
                Foreground="White" />
            <Button
                x:Name="PushPage"
                Content="Push Another Red Page"
                Foreground="White" />
            <Button
                x:Name="PopPage"
                Content="Pop Page"
                Foreground="White" />
            <Button
                x:Name="PopToRoot"
                Content="Pop To Root"
                Foreground="White" />
        </StackPanel>
    </views:ReactivePageRedView.Content>
</views:ReactivePageRedView>
